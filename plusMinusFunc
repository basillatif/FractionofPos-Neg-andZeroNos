import math
import os
import random
import re
import sys

# Complete the plusMinus function below.
def plusMinus(arr):
    n = len(arr)
    positive = sum(x > 0 for x in arr) / n
    negative = sum(x < 0 for x in arr) / n
    zero = sum (x == 0 for x in arr) /n
    print(positive, negative, zero, sep="\n")
